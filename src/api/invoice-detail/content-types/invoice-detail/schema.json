{
  "kind": "collectionType",
  "collectionName": "invoice_details",
  "info": {
    "singularName": "invoice-detail",
    "pluralName": "invoice-details",
    "displayName": "Invoice Detail"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "serviceName": {
      "type": "string",
      "required": true
    },
    "serviceDescription": {
      "type": "text"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "quantity": {
      "type": "integer",
      "default": 1,
      "required": true
    },
    "subtotal": {
      "type": "decimal",
      "required": true
    },
    "invoice": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::invoice.invoice",
      "inversedBy": "invoiceDetails"
    }
  }
}
